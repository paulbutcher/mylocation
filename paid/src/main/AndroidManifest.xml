<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionName="0.1"
          android:versionCode="1" 
          package="com.paulbutcher.mylocation"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="9"></uses-sdk>

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:icon="@drawable/android:star_big_on"
                 android:label="@string/app_name">

        <uses-library android:name="com.google.android.maps" />

        <activity android:label="@string/app_name"
                  android:name="com.paulbutcher.mylocation.library.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
    </application>
</manifest>
